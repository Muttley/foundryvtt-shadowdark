<div
	class="shadowdark chat-card roll-card"
	data-actor-id="{{actor.id}}"
	{{#if tokenId}}
		data-token-id="{{tokenId}}"
	{{/if}}
>
	<header class="card-header flexrow">
		<img src="{{actor.img}}" data-tooltip="{{title}}"/>
		<h3 class="roll-name">{{title}}</h3>
	</header>
	<div class="roll card-roll">
		{{#if check.HTML}}
			<div>{{{check.HTML}}}</div>
		{{/if}}
	</div>
</div>
